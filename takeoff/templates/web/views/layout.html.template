<!DOCTYPE html>
<html>
  <head>
    <title>{{generator.name}}</title>
  </head>

  <body>
    {{'{% if messages %}'}}
      {{ '{% for message in messages %}' }}
        <div{{ '{% if message.tags %}' }} class="{{ '{{ message.tags }}' }}"{{ '{% endif %}' }}>{{ '{{ message }}' }}</div>
      {{ '{% endfor %}' }}
    {{ '{% endif %}' }}
    {{ '{%block content %}{% endblock content %}' }}
  </body>
</html>
